@model List<AracKiralama.WebMvc.SoapService.tblArac>
@{
    ViewBag.Title = "Anasayfa";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<nav class="navbar navbar-expand-lg navbar-light bg-light">

    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav">
            <li class="nav-item active">
                <a href="/Anasayfa/kurumsalAnasayfa?sirketId=@Session["sirketId"]" class="nav-link">Ana Sayfa</a>
            </li>
            <li class="nav-item">
                <a  href="/Talep/talepler" class="nav-link">Talepler</a>
            </li>
            <li class="nav-item">
                <a class="nav-link">Kiralamalar</a>
            </li>
            <li class="nav-item">
                <a href="/Arac/aracEkle" class="nav-link">Araç Ekle</a>
            </li>
            <li class="nav-item">
                <a href="/Giris/Index" class="nav-link">Çıkış Yap</a>
            </li>
        </ul>
    </div>
</nav>




<div class="container">

    <div class="row">

        @foreach (AracKiralama.WebMvc.SoapService.tblArac arac in Model)
        {
            <div class="col-md-4 col-xs-12 col-lg 3 img-thumbnail urunler text-center">

                <form>

                    <div class="card w-100" style="width: 18rem;">
                        <img class="card-img-top" src="@arac.aracFotograf" alt="Card image cap">
                        <div class="card-body">
                            <h5 class="card-title"> @arac.aracMarka @arac.aracModel </h5>
                            <p class="card-text"> @arac.aracAciklama </p>

                        </div>
                        
                    </div>


                </form>

            </div>

        }

    </div>
</div>

